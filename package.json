{
  "name": "tpy-site",
  "version": "0.1.0",
  "description": "Technology Park Ypenburg on the Web",
  "main": "index.js",
  "scripts": {
    "dev-without-server": "next",
    "dev": "export $(cat .env | xargs) && npm run build && node api/server.js",
    "start": "npm install && NODE_ENV=production node api/server.js",
    "build": "./node_modules/next/dist/bin/next build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "next build && next export",
    "serve": "next build && next export && node api/server.js"
  },
  "plugins": [
    [
      "styled-jsx/babel",
      {
        "optimizeForSpeed": true
      },
      "babel-plugin-styled-components",
      {
        "ssr": true
      }
    ]
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bartwr/tpy-site.git"
  },
  "keywords": [
    "tpy"
  ],
  "author": "bartwr",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bartwr/tpy-site/issues"
  },
  "homepage": "https://github.com/bartwr/tpy-site#readme",
  "dependencies": {
    "@sendgrid/mail": "6.4.0",
    "chalk": "1.1.3",
    "cli-table2": "0.2.0",
    "contentful": "9.1.18",
    "dotenv": "^8.6.0",
    "express": "4.17.1",
    "framer-motion": "^6.2.8",
    "google-map-react": "2.1.10",
    "in-view": "0.6.1",
    "jquery": "3.5.0",
    "lodash": "^4.17.21",
    "mailchimp-api-v3": "1.13.1",
    "mailgun-js": "^0.22.0",
    "marked": "0.7.0",
    "moment": "2.24.0",
    "next": "12.1.1",
    "next-absolute-url": "^1.2.2",
    "node-fetch": "^2.6.5",
    "ramda": "0.28.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-swipe": "6.0.4",
    "sal.js": "^0.8.4",
    "sitemap": "2.2.0",
    "styled-components": "^5.2.1",
    "styled-jsx": "5.0.2",
    "swipe-js-iso": "2.1.5"
  },
  "engines": {
    "node": "14.15.4"
  },
  "devDependencies": {
    "babel-plugin-styled-components": "^1.12.0"
  }
}
